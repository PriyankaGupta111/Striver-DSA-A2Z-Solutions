class Solution {
    public void moveZeroes(int[] nums) {
        int i = 0; // Slow pointer - non-zero elements को track करता है
        
        // Step 1: Move all non-zero elements to the left side
        for (int j = 0; j < nums.length; j++) {
            if (nums[j] != 0) { 
                // Swap only when i and j are different
               
                    int temp = nums[j];
                    nums[j] = nums[i];
                    nums[i] = temp;
                
                i++; // Move slow pointer forward
            }
        }
    }
}
